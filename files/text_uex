<script>

	/* Open YouTube video preview */

	var ships_preview_video = function (url, name)
	{

		var el_body = $('body');
		var el_dialog = $('#dialog-preview-youtube');
		var el_embed = $('#ships_embed_youtube');
		var el_title = $('#title-preview-youtube');
		var class_panel = "youtube-preview-open";

		if (url)
		{

			var youtube_match = url.match(/^.*((youtu.be\/)|(v\/)|(\/u\/\w\/)|(embed\/)|(watch\?))\??v?=?([^#&?]*).*/);
			var youtube_id = (youtube_match && youtube_match[7].length==11) ? youtube_match[7] : false;

			if (!youtube_id)
			{
				alert("Error loading video. Please report this issue to UEX. Thank you.");
				return false;
			}

			var embed_url = `https://www.youtube.com/embed/${youtube_id}?autoplay=1&origin=https://uexcorp.space/ships/`;

		}

		if (is_mobile())
		{
			window.open(url);
		}
		else
		{

			if (!el_body.hasClass(class_panel))
			{
				el_body.addClass('dialog-show ' + class_panel);
				el_embed.attr('src', embed_url);
				el_title.html(name);
				el_embed.show();
				el_dialog.show();
			}
			else
			{
				el_body.removeClass('dialog-show ' + class_panel);
				el_embed.attr('src', '');
				el_title.html('');
				el_embed.hide();
				el_dialog.hide();
			}

		}

		return false;

	};

	var ships_toggle_section = function (row_class, el)
	{

		var class_name = 'row-hidden';

		el = $(el);
		row_class = $(`.tbl-ships tr.${row_class}`);

		if (!el.hasClass(class_name))
		{
			el.addClass(class_name);
			el.find('.fa').removeClass('fa-caret-down').addClass('fa-caret-right');
			row_class.hide();
		}
		else
		{
			el.removeClass(class_name);
			el.find('.fa').removeClass('fa-caret-right').addClass('fa-caret-down');
			row_class.show();
		}

	};

	var init_ships = function ()
	{

		$('.search-table').on('keyup', function()
		{

			var el = $('.brand-toggle');

			el.removeClass('row-hidden');
			el.find('.fa').removeClass('fa-caret-right').addClass('fa-caret-down');

			$('html, body').animate({ scrollTop: '0' }, 1);

		});

		$(document).keyup(function(e) 
		{
			if (e.key === "Escape") 
			{
				$('.search-table').val('').trigger('keyup');
			}
		});

		console.warn('[UEX] Ships tab: OK');

	};
	
</script>
						</div>

					</div>

				</div>

			</div>

		</main>

		